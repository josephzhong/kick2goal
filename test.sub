arguments = $(train_seed) $(test_seed)
executable = kick2goal/run.sh


log = job_$(train_seed).log
error = job_$(train_seed).err
output = job_$(train_seed).out

should_transfer_files = YES
when_to_transfer_output = ON_EXIT

transfer_input_files = kick2goal/

request_cpus = 9
request_memory = 16GB
request_disk = 1GB

queue train_seed,test_seed from kick2goal/seeds.txt